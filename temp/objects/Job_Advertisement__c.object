    <fields>
        <fullName>MAX_Salary__c</fullName>
        <externalId>false</externalId>
        <formula>IF(CONTAINS(TEXT(Salary__c),&quot;+&quot;),
    VALUE(&quot;9999999999999999&quot;),
    VALUE(TRIM(LEFT(MAX_Salary_helper__c, FIND(&quot;K&quot;, MAX_Salary_helper__c)-1))+&quot;000&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>MAX Salary</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>MAX_Salary_helper__c</fullName>
        <externalId>false</externalId>
        <formula>MID(
    TEXT(Salary__c),
    FIND(&quot;-&quot;, TEXT(Salary__c))+1,
    LEN(TEXT(Salary__c))-FIND(&quot;-&quot;, TEXT(Salary__c))
    )</formula>
        <label>MAX Salary helper</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
